<section>
    <h2 class="text-jade-powder uppercase font-medium">Skills</h2>
    <p class="font-normal text-lg lg:text-2xl">Multidisciplinary Front-end engineer</p>
    <ul class="mt-2.5 grid grid-cols-2  grid-flow-row gap-4 lg:grid-cols-4">
        {% for skill in skillList %}
            <li class="flex flex-1 flex-wrap items-center gap-x-4">
                <div class="w-[35px] h-[35px]">
                    <svg class="w-[35px] h-[35px]" aria-hidden="true"><use href="{{skill.icon}}"/></svg>
                </div>
                <p>{{skill.name}}</p>
            </li>

        {% endfor %}
    </ul>
</section>